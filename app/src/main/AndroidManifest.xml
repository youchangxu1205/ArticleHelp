<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.studyjams.s2.sj107.article">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cn.studyjams.s2.sj107.article.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cn.studyjams.s2.sj107.article.activity.AddArticleActivity"
            android:parentActivityName="cn.studyjams.s2.sj107.article.activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="cn.studyjams.s2.sj107.article.activity.MainActivity" />
        </activity>
        <activity
            android:name="cn.studyjams.s2.sj107.article.activity.UserDetailActivity"
            android:parentActivityName="cn.studyjams.s2.sj107.article.activity.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="cn.studyjams.s2.sj107.article.activity.MainActivity" />
        </activity>
    </application>

</manifest>